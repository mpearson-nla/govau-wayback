<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
       
<urlrewrite>
    <rule>
        <from>^(/(?!(invalidUrl|wayback|WEB-INF|favicon\.ico|js)).*)$</from>
        <set type="response-header" name="X-AGWA-Invalid-URL">yes</set>
        <to type="redirect">/invalidUrl$1?%{query-string}</to>
    </rule>

    <rule>
        <from>^(.*)\?$</from>
        <to type="redirect">$1</to>
    </rule>
</urlrewrite>

